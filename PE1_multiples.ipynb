   "source": [
    "Find the sum of all multiples of 3 or 5 below 1000."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "77d709a4-43fe-44dd-823f-f9314aac058d",
   "metadata": {},
   "outputs": [],
   "source": [
    "N = 1000\n",
    "\n",
    "multSum = 0\n",
    "for i in range(1,N):\n",
    "    if (i % 3 == 0) or (i % 5 == 0):\n",
    "        multSum += i"
   ]
